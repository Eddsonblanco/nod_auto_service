{
  "name": "nod_auto_service",
  "version": "1.0.0",
  "description": "CRASSA with Material-UI",
  "main": "index.js",
  "scripts": {
    "start": "crassa start",
    "start:dev": "crassa start-dev",
    "build": "crassa build",
    "dev": "crassa dev",
    "lint": "crassa lint",
    "count": "crassa count-lines",
    "prod": "yarn build && yarn start",
    "predeploy": "npm run build",
    "deploy": "gh-pages -b gh-pages -d build",
    "lint-staged": "lint-staged"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "crassa": {
    "displayName": "nod_auto_service"
  },
  "_moduleAliases": {
    "components": "./src/components",
    "containers": "./src/containers",
    "lib": "./src/lib",
    "reducers": "./src/reducers",
    "sources": "./src/sources",
    "utils": "./src/utils"
  },
  "jest": {
    "moduleNameMapper": {
      "utils": "<rootDir>/src/utils",
      "lib": "<rootDir>/src/lib",
      "reducers": "<rootDir>/src/reducers",
      "sagas": "<rootDir>/src/sagas",
      "components": "<rootDir>/src/components",
      "containers": "<rootDir>/src/containers",
      "api": "<rootDir>/src/api",
      "assets": "<rootDir>/src/assets"
    }
  },
  "dependencies": {
    "@ckeditor/ckeditor5-build-inline": "19.0.0",
    "@ckeditor/ckeditor5-react": "2.1.0",
    "@crassa/next": "0.9.30",
    "@date-io/date-fns": "1.3.11",
    "@hot-loader/react-dom": "16.13.0",
    "@material-ui/core": "4.10.0",
    "@material-ui/icons": "4.9.1",
    "@material-ui/pickers": "3.2.10",
    "@react-page/plugins-image": "0.21.1",
    "@react-page/plugins-spacer": "0.21.1",
    "@react-page/react-page": "0.21.1",
    "@react-page/renderer": "0.21.1",
    "add": "2.0.6",
    "axios": "0.21.1",
    "axios-debug-log": "0.7.0",
    "bcrypt": "4.0.1",
    "connected-react-router": "6.8.0",
    "date-fns": "2.14.0",
    "extensible-duck": "1.6.0",
    "google-maps-react": "2.0.6",
    "hero-slider": "2.0.3",
    "http-proxy-middleware": "1.0.4",
    "js-cookie": "2.2.1",
    "jsonwebtoken": "8.5.1",
    "mongoose": "5.9.16",
    "multer": "1.4.2",
    "node-sass": "4.14.1",
    "passport": "0.4.1",
    "passport-jwt": "4.0.0",
    "passport-local": "1.0.0",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-hook-form": "5.7.2",
    "react-hot-loader": "4.12.21",
    "react-png-button": "1.0.4",
    "react-redux": "7.2.0",
    "react-router-dom": "5.2.0",
    "react-scripts": "3.4.1",
    "react-slick": "0.26.1",
    "redux": "4.0.5",
    "redux-logger": "3.0.6",
    "redux-saga": "1.1.3",
    "uuid": "8.1.0",
    "video-react": "0.14.1",
    "yarn": "1.22.4"
  },
  "devDependencies": {
    "@testing-library/react": "10.0.2",
    "eslint-plugin-sort-keys-fix": "1.1.1",
    "gh-pages": "2.2.0",
    "react-hook-form-devtools": "1.2.0",
    "redux-devtools": "3.5.0",
    "redux-devtools-dock-monitor": "1.1.3",
    "redux-devtools-log-monitor": "1.4.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "{src,server}/**/*.js": [
      "eslint . --ext .js --fix",
      "git add"
    ]
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
